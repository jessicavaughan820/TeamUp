{
  "name": "teamup",
  "version": "1.0.0",
  "description": "An app to organize community sports teams",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./server/server.js",
    "test": "jest --verbose",
    "format": "prettier \"./**/*.{js,html}\" --write",
    "lint": "eslint \"./**/*.{js,jsx}\" --quiet",
    "build": "NODE_ENV=production webpack",
    "dev": "NODE_ENV=development nodemon ./server/server.js & webpack-dev-server --open",
    "db-up": "node ./server/db/execute-script.js ./server/db/create.sql",
    "db-down": "node ./server/db/execute-script.js ./server/db/destroy.sql"
  },
  "nodemonConfig": {
    "ignore": [
      "build",
      "client"
    ]
  },
  "author": "Jessica Vaughan",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "body-parser": "^1.19.0",
    "cjs-loader": "^0.1.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "pg": "^7.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4"
  }
}
